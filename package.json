{
    "name": "itypescript",
    "version": "0.1.0",
    "description": "ITypescript is a Typescript kernel for the Jupyter notebook",
    "keywords": [
        "typescript",
        "kernel",
        "ipython",
        "jupyter"
    ],
    "homepage": "https://nearbydelta.github.io/itypescript",
    "bugs": {
        "url": "https://github.com/nearbydelta/itypescript/issues"
    },
    "license": "BSD-3-Clause",
    "author": {
        "author": "Bugeun Kim"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nearbydelta/itypescript.git"
    },
    "bin": {
        "its": "bin/itypescript.ts",
        "itskernel": "lib/kernel.ts"
    },
    "dependencies": {
        "jp-kernel": "latest",
        "tsun": "latest"
    },
    "devDependencies": {
        "debug": "latest",
        "tsdoc": "latest",
        "tshint": "latest",
        "mocha": "3"
    },
    "scripts": {
        "doc": "python scripts/doc-build.py",
        "doc:publish": "node scripts/doc-publish.ts gh-pages https://github.com/nearbydelta/itypescript",
        "lint": "tshint bin lib",
        "test:mocha": "mocha",
        "test": "npm run lint && npm run test:mocha"
    }
}
